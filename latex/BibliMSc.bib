file = {:C$\backslash$:/Users/dev/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Chang, Yao, Li - 2017 - The design of a context-aware service system in intelligent transportation system.pdf:pdf},
journal = {International Journal of Distributed Sensor Networks},
keywords = {Context-aware service,context reasoning,intelligent service system,intelligent transportation systems,mobile information system},
mendeley-groups = {Contexto/Razonamiento},
number = {10},
title = {{The design of a context-aware service system in intelligent transportation system}},
url = {http://journals.sagepub.com/doi/pdf/10.1177/1550147717738165},
volume = {13},
