@article{Taub2011TheEscort,
abstract = {People living with Alzheimers and related dementias walk aimlessly for a variety of reasons; unchecked anxi-ety, little else to do, and boredom. This behavior is labeled " wandering. " We present a system to help caregivers en-sure the safety of residents who wander in assisted living environments. The Escort TM System uses Talking Lights R location-transmitting light beacons and a ZigBee R wire-less network to monitor the presence of residents in poten-tially dangerous areas. When the system learns that the patient is in a potential danger area, it sends a pager-or cellphone-based alert to on-duty caregivers. An evaluation of this system at an operating care facility is presented.},
author = {Taub, Daniel M and Leeb, Steven B and Lupton, Elmer C and Hinman, Roderick T and Zeisel, John and Blackler, Sue},
file = {:C$\backslash$:/Users/dev/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Taub et al. - Unknown - The Escort System A Safety Monitor for People Living with Alzheimer's Disease.pdf:pdf},
journal = {IEEE Pervasive Computing},
mendeley-groups = {Contexto/Alzheimer},
pages = {68----77},
title = {{The Escort System: A Safety Monitor for People Living with Alzheimer's Disease}},
url = {http://www.rle.mit.edu/esg/wp-content/uploads/2016/08/escort.pdf},
volume = {10},
year = {2011}
}

@article{Zolfaghari2016,
abstract = {Individuals with Autism Spectrum Disorder (ASD) can face great challenges in learning and maintaining basic living skills. This not only reduces their possibilities of independent living and employment, but also continuously brings social and financial burdens to their caregivers/mentors. Although research has been proposed to help autism users, most of them focus on improving social and communication skills or providing help for emergency situations. In this paper, we propose a novel portable context-aware assistive system to help autism users in their daily activities such as cooking and cleaning. To make it easily accessible and cost effective, we employ mobile devices and cheap context sensors. Our care system has been implemented and tested under different settings, and a user study involving ten pairs of autism users and their caregiver/mentors has been carried out to evaluate our approach. User feedback is highly positive. {\textcopyright} 2016 IEEE.},
