isbn = {9781467350778},
journal = {2013 IEEE International Conference on Pervasive Computing and Communications Workshops, PerCom Workshops 2013},
keywords = {OWL,SPARQL,activity recognition,context},
mendeley-groups = {Contexto/RazonamientoContext/SPARQL-RULES},
number = {March},
pages = {25--30},
title = {{SP-ACT: A hybrid framework for complex activity recognition combining OWL and SPARQL rules}},
year = {2013}
